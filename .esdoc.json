{
  "source": "./",
  "destination": "./docs",
  "includes": ["\\.js$"],
  "excludes": ["node_modules", "docs", "tests"],
  "index": "./README.md",
  "package.json": "./package.json",
  "plugins": [
    {"name": "esdoc-publish-html-plugin"},
    {
      "name": "esdoc-undocumented-identifier-plugin",
      "option": {"enable": false}
    },
    {
      "name": "esdoc-accessor-plugin",
      "option": {"access": ["public", "protected"]}
    },
    {
      "name": "esdoc-integrate-test-plugin",
      "option": {
        "source": "./tests",
        "interfaces": ["describe", "it"],
        "includes": ["test\\.js"]
      }
    }
  ]
}

