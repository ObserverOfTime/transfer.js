{
  "name": "transfer.js",
  "version": "0.4.0",
  "description": "Fork of transfer-sh, a CLI tool for easy file sharing with https://transfer.sh",
  "main": "./index.js",
  "files": [
    "bin/",
    "lib/"
  ],
  "preferGlobal": true,
  "bin": {
    "transfer-js": "./bin/transfer.js"
  },
  "scripts": {
    "cover": "nyc report -r text-lcov | codecov",
    "docs": "nyc report -r html; jsdoc -c .jsdoc.json",
    "lint": "eslint bin/*.js lib/*.js tests/*.js",
    "test": "nyc -r text-summary mocha -t 5000 tests/test.js --exit"
  },
  "repository": "git+https://github.com/ObserverOfTime/transfer.js.git",
  "keywords": [
    "transfer",
    "transfer.sh",
    "wrapper",
    "cli",
    "upload",
    "file",
    "sharing"
  ],
  "author": "ObserverOfTime <chronobserver@disroot.org>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ObserverOfTime/transfer.js/issues"
  },
  "homepage": "https://observeroftime.github.io/transfer.js",
  "dependencies": {
    "b64": "^4.0.0",
    "block-stream2": "^1.1.0",
    "cli-progress": "^2.0.0",
    "clipboardy": "^1.2.3",
    "end-of-stream": "^1.4.1",
    "got": "^9.2.1",
    "mime-types": "^2.1.20",
    "minimist": "^1.2.0",
    "progress-promise": "^0.0.6",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "@reconbot/jsdoc-theme": "^1.0.3",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.1.0",
    "eslint": "^5.1.0",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 60,
    "include": [
      "lib/*.js"
    ],
    "report-dir": "./docs/coverage",
    "cache": true
  }
}
