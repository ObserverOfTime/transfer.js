{
  "name": "transfer.js",
  "version": "0.2.1",
  "description": "Fork of transfer-sh, a CLI tool for easy file sharing with https://transfer.sh",
  "main": "index.js",
  "preferGlobal": true,
  "bin": {
    "transfer-js": "./bin/transfer.js"
  },
  "files": [
    "index.js",
    "bin/",
    "lib/"
  ],
  "scripts": {
    "gen-docs": "node ./node_modules/.bin/doxdox {.,bin,lib}/*.js -l Bootstrap -o docs/index.html",
    "lint": "node ./node_modules/.bin/eslint {.,bin,lib,tests}/*.js",
    "test": "node ./node_modules/.bin/mocha -t 3000 tests/test.js --exit"
  },
  "repository": "git+https://github.com/ObserverOfTime/transfer.js.git",
  "keywords": [
    "transfer",
    "transfer.sh",
    "wrapper",
    "cli",
    "upload",
    "file",
    "sharing"
  ],
  "author": "ObserverOfTime <chronobserver@disroot.org>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ObserverOfTime/transfer.js/issues"
  },
  "homepage": "https://observeroftime.github.io/transfer.js",
  "dependencies": {
    "b64": "^4.0.0",
    "block-stream2": "^1.1.0",
    "cli-progress": "^2.0.0",
    "clipboardy": "^1.1.0",
    "concat-stream": "^1.6.0",
    "end-of-stream": "^1.4.0",
    "got": "^8.3.1",
    "minimist": "^1.2.0",
    "progress-promise": "^0.0.6",
    "pump": "^3.0.0",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "doxdox": "^2.0.3",
    "eslint": "^5.0.1",
    "mocha": "^5.2.0"
  }
}
